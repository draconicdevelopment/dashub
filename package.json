{
  "name": "dashub",
  "version": "1.0.0",
  "description": "Dashboard used to manage your company",
  "author": "Martin KanÄ›ra",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.vue",
      "<rootDir>/pages/**/*.vue"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.4.6",
    "@nuxtjs/axios": "5.10.3",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@vue/composition-api": "0.5.0",
    "firebase": "7.14.2",
    "firebase-admin": "8.12.1",
    "muuri": "0.8.0",
    "nuxt": "2.12.2",
    "nuxt-composition-api": "0.6.0",
    "web-animations-js": "2.3.2"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "7.9.6",
    "@nuxt/typescript-build": "0.6.6",
    "@nuxtjs/eslint-config": "2.0.2",
    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@nuxtjs/eslint-module": "1.2.0",
    "@nuxtjs/stylelint-module": "3.2.2",
    "@nuxtjs/tailwindcss": "2.0.0",
    "@types/jest": "25.2.1",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "core-js": "2.6.11",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "0.5.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "lint-staged": "10.2.2",
    "node-sass": "4.14.0",
    "prettier": "1.19.1",
    "pug": "3.0.1",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.3.3",
    "ts-jest": "25.4.0",
    "vue-jest": "4.0.0-beta.2"
  }
}
